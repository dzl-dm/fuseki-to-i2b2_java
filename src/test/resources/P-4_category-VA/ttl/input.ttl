@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix : <http://data.dzl.de/ont/dwh#> .

:Root :topLevelNode :Concept .
:Concept a skos:Concept;
    skos:prefLabel "Concept"@en;
.

:Root :topLevelNode :CategoryConcept .
:CategoryConcept a skos:Concept;
    skos:prefLabel "CategoryConcept"@en;
.


:Concept2a a skos:Concept;
    skos:prefLabel "Child concept - L2 branch a"@en;
    skos:broader :Concept;
.
:Concept3a a skos:Concept;
    skos:prefLabel "Child concept - L3 branch a"@en;
    skos:broader :Concept2a;
    skos:notation "child3-notation1a";
.

:Concept2b a skos:Concept;
    skos:prefLabel "Child concept - L2 branch b"@en;
    skos:broader :CategoryConcept;
.
:Concept3b a skos:Concept;
    skos:prefLabel "Child concept - L3 branch b"@en;
    skos:broader :Concept2b;
    skos:notation "child3-notation1b";
.
